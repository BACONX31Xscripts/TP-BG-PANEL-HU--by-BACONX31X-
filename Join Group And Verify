local GroupId = 34767504
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- GUI oluşturma
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = player:WaitForChild("PlayerGui")

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 350, 0, 150)
Frame.Position = UDim2.new(0.5, -175, 0.5, -75)
Frame.BackgroundColor3 = Color3.new(0, 0, 0)
Frame.Parent = ScreenGui

local TitleLabel = Instance.new("TextLabel")
TitleLabel.Size = UDim2.new(1, -20, 0, 50)
TitleLabel.Position = UDim2.new(0, 10, 0, 10)
TitleLabel.BackgroundTransparency = 1
TitleLabel.TextColor3 = Color3.new(1, 1, 1)
TitleLabel.Text = "JOIN THE CG DEISG GROUP (by BACONX31X)"
TitleLabel.TextScaled = true
TitleLabel.Parent = Frame

local JoinButton = Instance.new("TextButton")
JoinButton.Size = UDim2.new(0, 150, 0, 50)
JoinButton.Position = UDim2.new(0.5, -75, 0, 70)
JoinButton.BackgroundColor3 = Color3.new(0, 0.6, 0)
JoinButton.TextColor3 = Color3.new(1, 1, 1)
JoinButton.Text = "I Joined the Group ✅"
JoinButton.Parent = Frame

local WarningLabel = Instance.new("TextLabel")
WarningLabel.Size = UDim2.new(1, -20, 0, 30)
WarningLabel.Position = UDim2.new(0, 10, 1, -35)
WarningLabel.BackgroundTransparency = 1
WarningLabel.TextColor3 = Color3.new(1, 0, 0)
WarningLabel.Text = ""
WarningLabel.TextScaled = true
WarningLabel.Parent = Frame

-- Script GUI (Örnek: teleport GUI) - Başlangıçta gizli
local ScriptGui = Instance.new("ScreenGui")
ScriptGui.Parent = player:WaitForChild("PlayerGui")
ScriptGui.Enabled = false

local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 400, 0, 250)
MainFrame.Position = UDim2.new(0.5, -200, 0.5, -125)
MainFrame.BackgroundColor3 = Color3.new(0, 0, 0)
MainFrame.Parent = ScriptGui

local InfoLabel = Instance.new("TextLabel")
InfoLabel.Size = UDim2.new(1, -20, 0, 40)
InfoLabel.Position = UDim2.new(0, 10, 0, 10)
InfoLabel.BackgroundTransparency = 1
InfoLabel.TextColor3 = Color3.new(1, 1, 1)
InfoLabel.Text = "Teleport / Bring"
InfoLabel.TextScaled = true
InfoLabel.Parent = MainFrame

local NameInput = Instance.new("TextBox")
NameInput.Size = UDim2.new(0, 250, 0, 40)
NameInput.Position = UDim2.new(0, 10, 0, 60)
NameInput.PlaceholderText = "Enter player name"
NameInput.TextColor3 = Color3.new(1, 1, 1)
NameInput.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)
NameInput.Parent = MainFrame

local TeleportButton = Instance.new("TextButton")
TeleportButton.Size = UDim2.new(0, 120, 0, 40)
TeleportButton.Position = UDim2.new(0, 10, 0, 110)
TeleportButton.BackgroundColor3 = Color3.new(0, 0.5, 1)
TeleportButton.TextColor3 = Color3.new(1, 1, 1)
TeleportButton.Text = "Teleport"
TeleportButton.Parent = MainFrame

local BringButton = Instance.new("TextButton")
BringButton.Size = UDim2.new(0, 120, 0, 40)
BringButton.Position = UDim2.new(0, 140, 0, 110)
BringButton.BackgroundColor3 = Color3.new(0, 0.7, 0.2)
BringButton.TextColor3 = Color3.new(1, 1, 1)
BringButton.Text = "Bring"
BringButton.Parent = MainFrame

-- GUI açıp kapatma için "-" tuşu (Minus)
local UserInputService = game:GetService("UserInputService")

UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.Minus then
        ScriptGui.Enabled = not ScriptGui.Enabled
    end
end)

-- Grup kontrol ve açma fonksiyonu
local function checkGroupAndOpen()
    local success, isInGroup = pcall(function()
        return player:IsInGroup(GroupId)
    end)

    if success and isInGroup then
        ScreenGui.Enabled = false
        ScriptGui.Enabled = true
    else
        WarningLabel.Text = "You have not joined the group! Failed ❌"
        wait(5)
        WarningLabel.Text = ""
    end
end

JoinButton.MouseButton1Click:Connect(checkGroupAndOpen)

-- Teleport fonksiyonu
TeleportButton.MouseButton1Click:Connect(function()
    local targetName = NameInput.Text
    if targetName == "" then return end

    local targetPlayer = Players:FindFirstChild(targetName)
    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
    end
end)

-- Bring fonksiyonu
BringButton.MouseButton1Click:Connect(function()
    local targetName = NameInput.Text
    if targetName == "" then return end

    local targetPlayer = Players:FindFirstChild(targetName)
    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        targetPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
    end
end)
